<AuFieldset as |f|>
  <f.legend>{{@label}}</f.legend>
  <f.content>
    <AuCheckboxGroup
      @name={{@label}}
      @selected={{this.selectedIds}}
      @onChange={{this.updateSelection}} as |Group|>
      {{#each this.concepts as |concept|}}
        <Group.Checkbox @value={{concept.id}}>{{concept.label}}</Group.Checkbox>
      {{/each}}
    </AuCheckboxGroup>
  </f.content>
</AuFieldset>
